@article{Chu2010,
	Abstract = {Considers the need to have a set of indicators for measuring democracy that are reliable across diverse societies. The puzzling outcomes generated by some indicators when they cross borders are described. Results of the 2001 & 2006 Asian Barometer Surveys (ABSs) are analyzed to show that people have different notions of the meaning of democracy. A support for democracy sometimes indicates how respondents want to be perceived by others rather than whether they understand what democracy is. It is contended that researchers should never rely on a single indicator to measure democratic legitimacy across nations & should "cross-examine the validity of all indicators based on the word `democracy'." ABS's inclusion of other attitudinal dimensions & an additive battery of questions to investigate the substance/depth of a respondent's commitment to democracy is examined. Both measurements are combined to produce a "typology of democratic orientation" that describes four distinct types from "Consistent democrats" to "Superficial democrats." An analysis of the results across 13 Asian countries shows that the typology resolves problems resulting from inconsistent understandings of democracy. J. Lindroth},
	Author = {Chu, Yun-han and Huang, Min-hua},
	Endnotereftype = {Journal Article},
	Issn = {1045-5736, 1045-5736},
