	Isbn = {00030554},
	Journal = {American Political Science Review},
	M1 = {ArticleType: primary{\_}article / Full publication date: Mar., 2001 / Copyright {\copyright}2001 American Political Science Association},
	Number = {1},
	Pages = {49--69},
	Publisher = {American Political Science Association},
	Title = {Analyzing Incomplete Political Science Data: An Alternative Algorithm for Multiple Imputation},
	Ty = {JOUR},
	Volume = {95},
	Year = {2001},
