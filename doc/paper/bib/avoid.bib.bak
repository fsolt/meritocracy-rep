@Article{Monogan2015,
  Title                    = {Research Preregistration in Political Science: The Case, Counterarguments, and a Response to Critiques},
  Author                   = {Monogan, James E. III},
  Journal                  = {PS: Political Science \& Politics},
  Year                     = {2015},
  Month                    = {7},
  Number                   = {3},
  Pages                    = {425--429},
  Volume                   = {48},
  Abstract                 = { ABSTRACT ABSTRACT            This article describes the current debate on the practice of preregistration in political science‚{\"A}{\^\i}that is, publicly releasing a research design before observing outcome data. The case in favor of preregistration maintains that it can restrain four potential causes of publication bias, clearly distinguish deductive and inductive studies, add transparency regarding a researcher‚{\"A}{\^o}s motivation, and liberate researchers who may be pressured to find specific results. Concerns about preregistration maintain that it is less suitable for the study of historical data, could reduce data exploration, may not allow for contextual problems that emerge in field research, and may increase the difficulty of finding true positive results. This article makes the case that these concerns can be addressed in preregistered studies, and it offers advice to those who would like to pursue study registration in their own work. },
  Bdsk-url-1               = {http://journals.cambridge.org/article_S1049096515000189},
  Bdsk-url-2               = {http://dx.doi.org/10.1017/S1049096515000189},
  Date-added               = {2015-11-19 15:38:39 +0000},
  Date-modified            = {2015-11-19 15:39:25 +0000},
  URL                      = {http://journals.cambridge.org/article_S1049096515000189}
}

@Article{Newman2015,
  Title                    = {False Consciousness or Class Awareness? Local Income Inequality, Personal Economic Position, and Belief in American Meritocracy},
  Author                   = {Newman, Benjamin J. and Johnston, Christopher D. and Lown, Patrick L.},
  Journal                  = {American Journal of Political Science},
  Year                     = {2015},
  Number                   = {2},
  Pages                    = {326--340},
  Volume                   = {59},
  Abstract                 = {Existing research analyzes the effects of cross-national and temporal variation in income inequality on public opinion; however, research has failed to explore the impact of variation in inequality across citizens' local residential context. This article analyzes the impact of local inequality on citizens' belief in a core facet of the American ethos---meritocracy. We advance conditional effects hypotheses that collectively argue that the effect of residing in a high-inequality context will be moderated by individual income. Utilizing national survey data, we demonstrate that residing in more unequal counties heightens rejection of meritocracy among low-income residents and bolsters adherence among high-income residents. In relatively equal counties, we find no significant differences between high- and low-income citizens. We conclude by discussing the implications of class-based polarization found in response to local inequality with respect to current debates over the consequences of income inequality for American democracy.},
  Bdsk-file-1              = {},
  Bdsk-url-1               = {http://dx.doi.org/10.1111/ajps.12153},
  Date-added               = {2015-10-28 20:53:30 +0000},
  Date-modified            = {2015-10-28 20:53:30 +0000},
  Keywords                 = {Inequality}
}

